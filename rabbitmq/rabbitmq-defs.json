{
    "rabbit_version": "3.6.10",
    "users": [
        {
            "name": "admin",
            "password": "password",
            "tags": "administrator"
        },
        {
            "name": "monitor",
            "password_hash": "dfvC76mPSc4yyu9GyNLZOaqPMR3y6Atkxm/kND2bBa4mAt5z",
            "hashing_algorithm": "rabbit_password_hashing_sha256",
            "tags": "monitoring"
        },
        {
            "name": "some-application",
            "password": "password",
            "tags": "none"
        }
    ],
    "vhosts": [
        {
            "name": "/"
        }
    ],
    "permissions": [
        {
            "user": "admin",
            "vhost": "/",
            "configure": ".*",
            "write": ".*",
            "read": ".*"
        },
        {
            "user": "some-application",
            "vhost": "/",
            "configure": "^some\\..*",
            "write": ".*",
            "read": ".*"
        }
    ],
    "parameters": [],
    "global_parameters": [],
    "policies": [
        {
          "vhost": "/",
          "name": "ha-all",
          "pattern": ".*",
          "apply-to": "all",
          "definition": {
            "ha-mode": "all",
            "ha-sync-mode": "automatic"
          },
          "priority": 1
      },
        {
          "vhost": "/",
          "name": "ha-and-dlx",
          "pattern": ".*",
          "apply-to": "queues",
          "definition": {
            "dead-letter-exchange": "DLX",
            "ha-mode": "all",
            "ha-sync-mode": "automatic"
          },
          "priority": 10
        }
    ],
    "queues": [
        {
            "name": "DLQ",
            "vhost": "/",
            "durable": true,
            "auto_delete": false,
            "arguments": {}
        }
    ],
    "exchanges": [
        {
            "name": "some.exchange",
            "vhost": "/",
            "type": "direct",
            "durable": true,
            "auto_delete": false,
            "internal": false,
            "arguments": {}
        },
        {
            "name": "DLX",
            "vhost": "/",
            "type": "fanout",
            "durable": true,
            "auto_delete": false,
            "internal": false,
            "arguments": {}
        }
    ],
    "bindings": [
        {
            "source": "DLX",
            "vhost": "/",
            "destination": "DLQ",
            "destination_type": "queue",
            "arguments": {}
        }
    ]
}
